{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/keystatic(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/articles/?$",
			"dest": "_render"
		},
		{
			"src": "^/contato/?$",
			"dest": "_render"
		},
		{
			"src": "^/keystatic(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/politica-cookies/?$",
			"dest": "_render"
		},
		{
			"src": "^/privacidade/?$",
			"dest": "_render"
		},
		{
			"src": "^/rss\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/sobre/?$",
			"dest": "_render"
		},
		{
			"src": "^/termos-de-uso/?$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}